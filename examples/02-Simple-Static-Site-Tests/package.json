{
  "name": "02-simple-static-site-tests",
  "version": "1.0.0",
  "description": "Here's a simple static website that can be opened in StackBlitz. It supports Vitest/JSDom testing for vanilla JavaScript.",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "keywords": [],
  "author": "Dan Gilleland",
  "license": "ISC",
  "devDependencies": {
    "@vitest/ui": "^0.15.2",
    "jsdom": "^20.0.0",
    "vite": "^2.9.12",
    "vitest": "^0.15.2"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  }
}
